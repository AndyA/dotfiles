// \$Id\$

/**
 * Implementation of hook_install
 */
function [[%tabstop:example]]_install() {
  switch ($GLOBALS['db_type']) {
    case 'mysqli':
    case 'mysql':
      db_query("CREATE TABLE IF NOT EXISTS {[[%tabstop:example]]} (
        id int unsigned NOT NULL auto_increment,
        field varchar(128) NOT NULL default ''
        PRIMARY KEY (id),
      ) /*!40100 DEFAULT CHARACTER SET utf8 */;");
    break;
  }
}


/**
 * Implementation of hook_uninstall
 */
function [[%tabstop:example]]_uninstall() {
  db_query("DROP TABLE {[[%tabstop:example]]}");
  $variables = db_query("SELECT name FROM {variable} WHERE name LIKE '[[%tabstop:example]]%%'");
  while ($variable = db_fetch_object($variables)) {
    variable_del($variable-&gt;name);
  }
}!@#_currentPos!@#_anchor</snippet>
