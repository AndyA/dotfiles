# local
PATH=/opt/local/bin:/usr/local/bin:/usr/local/mysql/bin:$PATH
PATH=/usr/local/apache2/bin:/usr/local/apache2/php/bin:$PATH
export CLICOLOR=1
export COPY_EXTENDED_ATTRIBUTES_DISABLE=1
export COPYFILE_DISABLE=1
export LC_CTYPE='en_US.UTF-8'
[ -e /Applications/MacPorts/Vim.app ] && alias gvim="/Applications/MacPorts/Vim.app/Contents/MacOS/Vim -g"
# Having a MANPATH set overrides man's searching heuristic
export MANPATH=
