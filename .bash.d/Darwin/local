# local
for d in \
  /usr/local/bin \
  /usr/local/apache2/bin \
  /usr/local/apache2/php/bin \
  /Developer/usr/bin \
  /Applications/Xcode.app/Contents/Developer/usr/bin; do
  [ -d "$d" ] && PATH="$d:$PATH"
done
export CLICOLOR=1
export COPY_EXTENDED_ATTRIBUTES_DISABLE=1
export COPYFILE_DISABLE=1
export LC_CTYPE='en_US.UTF-8'
# Having a MANPATH set overrides man's searching heuristic
export MANPATH=
