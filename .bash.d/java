# Java path settings
for java in /usr/local/java /usr/java ; do
  for dir in jdk jre ; do
    for bindir in $java/$dir*/bin ; do
      # FIXME: Adds multiple versions and probably adds them in
      # the wrong order.
      if [ -d $bindir ] ; then
        PATH=$PATH:$bindir
        break 3
      fi
    done
  done
done
