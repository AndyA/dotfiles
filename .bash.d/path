# Path settings
PATH=$HOME/bin:$PATH
# Add to the end of the path
for d in /usr/sbin /sbin; do
  [ -d "$d" ] && PATH="$PATH:$d"
done
# Add to the start of the path
for d in /usr/local/bin /usr/local/sbin $HOME/bin/local $HOME/bin/$RCUNAME; do
  [ -d "$d" ] && PATH="$d:$PATH"
done

# vim:ts=2:sw=2:sts=2:et:ft=sh
