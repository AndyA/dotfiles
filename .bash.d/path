# Path settings
PATH=$HOME/bin:$PATH
# Add to the end of the path
for d in /usr/local/sbin /usr/sbin /sbin; do
  [ -d $d ] && PATH=$PATH:$d
done
# Add to the start of the path
for d in $HOME/bin/local $HOME/bin/$RCUNAME \
  /usr/local/flexsdk/bin /usr/local/apache-maven/bin; do
  [ -d $d ] && PATH=$d:$PATH
done

