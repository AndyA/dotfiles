if [ -d ~/Dropbox ]; then
  alias pbput="pbpaste > ~/Dropbox/dotfiles/clipboard"
  alias pbget="cat ~/Dropbox/dotfiles/clipboard | pbcopy"
  for d in ~/Dropbox/bin ~/Dropbox/bin/$RCUNAME; do
    [ -d $d ] && PATH=$d:$PATH
  done
  hostdir=~/Dropbox/hosts/$(hostname -s)
  mkdir -p $hostdir
fi

