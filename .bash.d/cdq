function cdq() {
  eval $( private-cdq-helper "$@" )
}

function _cdq() {
  local IFS=$'\n'
  COMPREPLY=(
    $( private-cdq-helper -c "${COMP_WORDS[@]}" )
  )
}

function pinq() {
  local target="$PWD"
  local name=${1:-"$(basename "$target")"}
  local q="$HOME/.q/local"
  mkdir -p "$q"
  pushd "$q" > /dev/null 2>&1
  ln -s "$target" "$name"
  popd > /dev/null 2>&1
}

complete -F _cdq cdq

