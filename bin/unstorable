#!/usr/bin/env perl

use strict;
use warnings;

use Data::Dumper;
use Storable;

for my $st ( @ARGV ) {
  my $d = retrieve $st or die "Can't retrieve $st: $!\n";
  print fmt($d), "\n";
}

sub fmt {
  Data::Dumper->new( \@_ )->Indent( 2 )->Quotekeys( 0 )->Sortkeys( 1 )
   ->Useqq( 1 )->Terse( 1 )->Dump;
}

# vim:ts=2:sw=2:sts=2:et:ft=perl

